{
  "name": "forevercore-gdps",
  "version": "2.0.0",
  "type": "module",
  "main": "src/server.ts",
  "scripts": {
    "dev": "bun --watch src/server.ts",
    "build": "bun build src/server.ts --outdir dist --target bun",
    "start": "bun dist/server.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "dev:panel": "cd panelui && bun run dev",
    "build:panel": "cd panelui && bun run build",
    "dev:full": "concurrently \"bun run dev\" \"bun run dev:panel\""
  },
  "keywords": ["gdps", "geometry-dash", "private-server"],
  "author": "Fimast Arc.",
  "license": "ISC",
  "description": "ForeverCore GDPS - Modern TypeScript implementation",
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/multipart": "^9.1.0",
    "@fastify/static": "^8.0.2",
    "@fastify/cookie": "^10.0.1",
    "@fastify/formbody": "^8.0.1",
    "@fastify/redis": "^7.0.1",
    "fastify": "^5.2.0",
    "drizzle-orm": "^0.36.5",
    "mysql2": "^3.11.3",
    "redis": "^5.5.6",
    "bcryptjs": "^2.4.3",
    "zod": "^3.25.74",
    "@sinclair/typebox": "^0.34.6",
    "youtube-dl-exec": "^3.0.10",
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "drizzle-kit": "^0.30.2",
    "concurrently": "^8.2.2",
    "typescript": "^5.8.3"
  },
  "trustedDependencies": [
    "youtube-dl-exec"
  ]
}